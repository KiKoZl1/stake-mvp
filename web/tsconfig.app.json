{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "svelte",
      "vite/client",
      "node"
    ],
    "strict": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": false,
    "useDefineForClassFields": true,
    "baseUrl": ".",
    "paths": {
      "components-layout": [
        "../packages/components-layout/index.ts"
      ],
      "components-pixi": [
        "../packages/components-pixi/index.ts"
      ],
      "components-shared": [
        "../packages/components-shared/index.ts"
      ],
      "components-storybook": [
        "../packages/components-storybook/index.ts"
      ],
      "components-ui-html": [
        "../packages/components-ui-html/index.ts"
      ],
      "components-ui-pixi": [
        "../packages/components-ui-pixi/index.ts"
      ],
      "constants-shared": [
        "../packages/constants-shared/index.ts"
      ],
      "envs": [
        "../packages/envs/index.ts"
      ],
      "pixi-svelte": [
        "../packages/pixi-svelte/index.ts"
      ],
      "pixi-svelte-storybook": [
        "../packages/pixi-svelte-storybook/index.ts"
      ],
      "rgs-fetcher": [
        "../packages/rgs-fetcher/index.ts"
      ],
      "rgs-requests": [
        "src/rgs/requests.ts"
      ],
      "state-shared": [
        "../packages/state-shared/index.ts"
      ],
      "utils-bet": [
        "../packages/utils-bet/index.ts"
      ],
      "utils-book": [
        "../packages/utils-book/index.ts"
      ],
      "utils-event-emitter": [
        "../packages/utils-event-emitter/index.ts"
      ],
      "utils-fetcher": [
        "../packages/utils-fetcher/index.ts"
      ],
      "utils-layout": [
        "../packages/utils-layout/index.ts"
      ],
      "utils-resize-observer": [
        "../packages/utils-resize-observer/index.ts"
      ],
      "utils-shared": [
        "../packages/utils-shared/index.ts"
      ],
      "utils-slots": [
        "../packages/utils-slots/index.ts"
      ],
      "utils-sound": [
        "../packages/utils-sound/index.ts"
      ],
      "utils-xstate": [
        "../packages/utils-xstate/index.ts"
      ],
      "$env/static/public": [
        "src/env/public.ts"
      ],
      "rgs-requests-remote": [
        "../packages/rgs-requests/index.ts"
      ],
      "config-lingui": [
        "../packages/config-lingui/index.ts"
      ],
      "config-storybook": [
        "../packages/config-storybook/index.ts"
      ],
      "config-svelte": [
        "../packages/config-svelte/index.js"
      ],
      "config-vite": [
        "../packages/config-vite/index.js"
      ],
      "config-ts": [
        "../packages/config-ts/base.json"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.svelte",
    "src/**/*.d.ts"
  ]
}