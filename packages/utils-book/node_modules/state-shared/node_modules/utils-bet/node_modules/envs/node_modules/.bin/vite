#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/vite@6.2.0_@types+node@24.0_a83df7e5973aba2970dfbc76448e00ec/node_modules/vite/bin/node_modules:/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/vite@6.2.0_@types+node@24.0_a83df7e5973aba2970dfbc76448e00ec/node_modules/vite/node_modules:/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/vite@6.2.0_@types+node@24.0_a83df7e5973aba2970dfbc76448e00ec/node_modules:/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/vite@6.2.0_@types+node@24.0_a83df7e5973aba2970dfbc76448e00ec/node_modules/vite/bin/node_modules:/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/vite@6.2.0_@types+node@24.0_a83df7e5973aba2970dfbc76448e00ec/node_modules/vite/node_modules:/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/vite@6.2.0_@types+node@24.0_a83df7e5973aba2970dfbc76448e00ec/node_modules:/mnt/c/Users/KiKoZl/Downloads/TESTESDK/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
